<div id="DivLista">
    <h1>Hola!!</h1>
</div>
    <script src="/socket.io/socket.io.js" type="text/javascript"></script>
<script>
const socket = io()
console.log("Hola!")
let div = document.getElementById("DivLista")
div.innerHTML =''
let contenidoDiv =''


socket.on("updateProduct",data =>{
    console.log(data)
    contenidoDiv =''
    data.forEach((i)=>{
        contenidoDiv+= i.title +'\n'
    })
    
    div.innerHTML =''
    div.innerHTML = contenidoDiv
})

</script>